{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "semi": 1,
        "max-len": 0,
        "no-underscore-dangle": 0,
        "no-console": 1,
        "prefer-destructuring": 0,
        "camelcase": 0,
        "consistent-return": 0,
        "curly": 1,
        "no-alert": 1,
        "no-eval": 1,
        "no-useless-concat": 1
    }
}