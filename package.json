{
    "name": "tendered-api",
    "version": "1.0.0",
    "description": "Tendered API",
    "engines": {
        "node": "10.16.3"
    },
    "scripts": {
        "check-coverage": "istanbul cover node_modules/mocha/bin/_mocha -- src/tests/index.js --opts src/tests/mocha.opts",
        "coverage": "nyc mocha src/tests/index.js ",
        "cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- src/tests/index.js --opts src/tests/mocha.opts --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
        "coverage:coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "lint": "eslint src/controllers/*.js ./tests/*.js --fix",
        "fix-lint": "eslint . --fix",
        "test": "yarn run check-coverage",
        "startdev": "nodemon ./bin/www",
        "start": "node ./bin/www",
        "check-libs": "snyk test --dev"
    },
    "main": "index.js",
    "license": "MIT",
    "dependencies": {
        "agenda": "^2.1.0",
        "agendash": "^1.0.0",
        "ajv": "^6.10.2",
        "ajv-errors": "^1.0.1",
        "app-root-path": "^3.0.0",
        "await-of": "^1.2.0",
        "bcrypt": "^3.0.6",
        "boom": "^7.3.0",
        "check-dependencies": "^1.1.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.0.0",
        "helmet": "^3.21.2",
        "http-status-codes": "^1.4.0",
        "jsonwebtoken": "^9.0.0",
        "loadash": "^1.0.0",
        "moment": "^2.24.0",
        "mongoose": "^5.7.8",
        "morgan": "^1.9.1",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "random-int": "^2.0.1",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@bugsnag/js": "^6.4.3",
        "@bugsnag/plugin-express": "^6.4.1",
        "chai": "^4.2.0",
        "coveralls": "^3.0.9",
        "debug": "^4.1.1",
        "eslint": "^6.6.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.18.2",
        "faker": "^4.1.0",
        "istanbul": "^1.0.0-alpha.2",
        "mocha": "^6.2.2",
        "mocha-lcov-reporter": "^1.3.0",
        "nyc": "^14.1.1",
        "supertest": "^4.0.2"
    }
}
