{
    "name": "tendered-api",
    "version": "1.0.0",
    "description": "Tendered API",
    "engines": {
        "node": "12.12.0"
    },
    "scripts": {
        "check-coverage": "istanbul check-coverage",
        "coverage_": "istanbul cover node_modules/mocha/bin/_mocha -- ./test/index.test.js --opts ./test/mocha.opts",
        "coverage": "nyc mocha ./test/index.test.js --opts ./test/mocha.opts",
        "lint": "eslint src/controllers/*.js test/*.js --fix",
        "fix-lint": "eslint . --fix",
        "test": "yarn run lint && yarn run fix-lint && yarn run coverage && yarn run check-coverage",
        "newtest_": "yarn run coverage && yarn run check-coverage",
        "newtest": "yarn run coverage",
        "startdev": "nodemon ./bin/www",
        "start": "node ./bin/www",
        "check-libs": "snyk test --dev"
    },
    "main": "index.js",
    "license": "MIT",
    "dependencies": {
        "ajv": "^6.10.2",
        "ajv-errors": "^1.0.1",
        "app-root-path": "^3.0.0",
        "await-of": "^1.2.0",
        "bcrypt": "^3.0.6",
        "boom": "^7.3.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.0.0",
        "helmet": "^3.21.2",
        "http-status-codes": "^1.4.0",
        "jsonwebtoken": "^8.5.1",
        "loadash": "^1.0.0",
        "mongoose": "^5.7.8",
        "morgan": "^1.9.1",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@bugsnag/js": "^6.4.3",
        "@bugsnag/plugin-express": "^6.4.1",
        "chai": "^4.2.0",
        "debug": "^4.1.1",
        "eslint": "^6.6.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.18.2",
        "istanbul": "^0.4.5",
        "mocha": "^6.2.2",
        "nyc": "^14.1.1",
        "supertest": "^4.0.2"
    }
}
